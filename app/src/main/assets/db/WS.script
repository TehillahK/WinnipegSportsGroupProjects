CREATE MEMORY TABLE PUBLIC.NEWS_FEED(POSTID INTEGER NOT NULL PRIMARY KEY, TITLE VARCHAR(20),DATEPOSTED VARCHAR(20),CAPTION VARCHAR(20),NUMLIKES Integer,IMAGE Integer)
