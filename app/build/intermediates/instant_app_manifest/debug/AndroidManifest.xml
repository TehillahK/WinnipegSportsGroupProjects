            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="comp3350.winSport.presentation.MainActivity" >
        </activity>
        <activity android:name="comp3350.winSport.presentation.RosterActivity" />
        <activity android:name="comp3350.winSport.presentation.CurrentlyPlayingActivity" />
